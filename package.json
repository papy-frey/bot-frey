{
  "name": "projet",
  "version": "0.1.0",
  "description": "Projet Bot Discord",
  "main": "main.js",
  "repository": "git@github.com:papy-frey/bot-frey.git",
  "author": "Papy Frey <pouchat@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "discord.js": "discordjs/discord.js",
    "mongoose": "^5.6.4",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "eslint": "^6.0.1",
    "eslint-config-aqua": "^6.0.0"
  },
  "eslintConfig": {
    "extends": "aqua/node",
    "rules": {
      "linebreak-style": [
        "error",
        "windows"
      ],
      "quotes": [
        "warn",
        "double"
      ],
      "indent": [
        "error",
        2
      ],
      "curly": "off",
      "no-trailing-spaces": "off",
      "prefer-template": "off",
      "no-eval": "off"
    }
  }
}